{
  "name": "patternfly-sandbox-ng-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "npm run clean:lib && npm run clean:app",
    "clean:lib": "npm run rimraf -- projects/patternfly-sandbox-ng/node_modules",
    "clean:app": "npm run rimraf -- node_modules dist",
    "install:lib": "cd projects/patternfly-sandbox-ng && npm i",
    "build": "npm run build:lib && npm run build:app",
    "build:app": "npm run docs && ng build --prod",
    "prebuild:lib": "npm run install:lib && mkdir -p projects/patternfly-sandbox-ng/node_modules/patternfly/dist/less/bootstrap/less && cp -rf projects/patternfly-sandbox-ng/node_modules/bootstrap/less/* projects/patternfly-sandbox-ng/node_modules/patternfly/dist/less/bootstrap/less && mkdir -p projects/patternfly-sandbox-ng/node_modules/patternfly/dist/less/font-awesome/less && cp projects/patternfly-sandbox-ng/node_modules/font-awesome/less/*.less projects/patternfly-sandbox-ng/node_modules/patternfly/dist/less/font-awesome/less/",
    "build:lib": "ng build --prod patternfly-sandbox-ng",
    "test": "npm run test:lib",
    "pretest": "npm run build:lib",
    "test:app": "ng test --watch=false",
    "test:lib": "ng test patternfly-sandbox-ng --watch=false --browsers ChromeHeadlessNoSandbox",
    "test:lib:dev": "ng test patternfly-sandbox-ng",
    "rimraf": "rimraf",
    "pack:lib": "cd dist/patternfly-sandbox-ng && npm pack",
    "package": "npm run build:lib && npm run pack:lib",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./projects/patternfly-sandbox-ng/src/",
    "typedoc": "typedoc",
    "ts:lint": "tslint -c tslint.json 'src/**/*.ts'",
    "ts:lint:fix": "npm run ts:lint -- --fix",
    "semantic-release": "semantic-release pre && copy package.json npm-shrinkwrap.json dist && npm publish dist && semantic-release post",
    "semantic-release-post": "semantic-release post",
    "semantic-release-pre": "semantic-release pre",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/patternfly-sandbox-ng-demo/stats.json"
  },
  "license": "Apache-2.0",
  "contributors": [],
  "keywords": [
    "patternfly",
    "patternfly-ng",
    "patternfly-sandbox-ng",
    "bootstrap",
    "angular",
    "angular2",
    "angular 2",
    "angular4",
    "angular 4",
    "angular5",
    "angular 5",
    "angular6",
    "angular 6",
    "ng",
    "ng2",
    "ng4",
    "ng5",
    "ng6"
  ],
  "homepage": "https://github.com/patternfly/patternfly-sandbox-ng",
  "repository": {
    "type": "git",
    "url": "https://github.com/patternfly/patternfly-sandbox-ng.git"
  },
  "bugs": {
    "url": "https://github.com/patternfly/patternfly-sandbox-ng/issues"
  },
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=5.6.0"
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26",
    "lodash": "^4.17.10",
    "patternfly": "^3.28.0",
    "patternfly-ng": "^4.0.0"
  },
  "optionalDependencies": {
    "angular-tree-component": "^7.0.0",
    "ng2-dragula": "^1.5.0",
    "ngx-bootstrap": "^3.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "@angular/cli": "~6.1.3",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "highlight.js": "9.12.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "markdown-it": "8.4.1",
    "ng-packagr": "^4.1.1",
    "protractor": "~5.3.0",
    "semantic-release": "15.6.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typedoc": "0.11.1",
    "typescript": "~2.7.2",
    "webpack-bundle-analyzer": "^3.0.1"
  }
}
